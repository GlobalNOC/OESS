<configuration>
<interfaces>
  <interface>
    <name>[% interface.name %]</name>
    <unit operation='delete'>
      <name>[% vlan_tag %]</name>
    </unit>
  </interface>
</interfaces>
<protocols>
  <mpls>
    <label-switched-path operation='delete'>
      <name>L2VPLS-PRIMARY-LSP-[% circuit_id %]</name>
    </label-switched-path>
    <label-switched-path operation='delete'>
      <name>L2VPLS-SECONDARY-LSP-[% circuit_id %]</name>
    </label-switched-path>
    <path operation='delete'>
      <name>L2VPLS-PRIMARY-PATH-[% circuit_id %]</name>
    </path>
    <path operation='delete'>
      <name>L2VPLS-SECONDARY-PATH-[% circuit_id %]</name>
    </path>
  </mpls>
</protocols>
<policy-options>
  <policy-statement>
  <name>L2VPLS-LSP-Policy</name>
    <term operation='delete'>
      <name>L2VPLS-[% circuit_id %]</name>
    </term>
  </policy-statement>
  <community operation='delete'>
    <name>L2VPLS-[% circuit_id %]-Community</name>
  </community>
</policy-options>
<routing-instances>
  <instance operation='delete'>
    <name>L2VPLS-[% circuit_id %]</name>
  </instance>
</routing-instances>
</configuration>
